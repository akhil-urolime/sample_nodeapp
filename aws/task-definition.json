
{
    "family": "pod-task",
    "containerDefinitions": [
        {
            "image": "753233110933.dkr.ecr.us-west-1.amazonaws.com/podchaser:%BUILD_TAG%",
            "name": "pod-task",
            "essential": true,
            "cpu": 100,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
